<h1>{{title}}</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Matrícula</th>
      <th scope="col">Nome</th>
      <th scope="col">Data de Nascimento</th>
      <th scope="col">Endereço de Email</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    {{#each alunos.content}}
    <tr>
      <td>{{matricula}}</td>
      <td><a href="/alunos/{{matricula}}">{{nome}}</a></td>
      <td>{{data_nascimento}}</td>
      <td>{{email}}</td>
      <td><a href="/alunos/edit/{{matricula}}">Editar</a></td>
    </tr>
    {{/each}}
  </tbody>
</table>

<a href="/alunos/new">Adicionar aluno</a>