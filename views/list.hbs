<h1>{{title}}</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Matrícula</th>
      <th scope="col">Nome</th>
      <th scope="col">Data de Nascimento</th>
      <th scope="col">Endereço de Email</th>
    </tr>
  </thead>

  <tbody>

    {{#each alunos.content}}
    <tr>
      <td>{{matricula}}</td>
      <td>{{nome}}</td>
      <td>{{data_nascimento}}</td>
      <td>{{email}}</td>
    </tr>
    {{/each}}

  </tbody>
</table>