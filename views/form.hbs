<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/alunos">Alunos</a></li>
    <li class="breadcrumb-item active" aria-current="page">{{title}}</li>
  </ol>
</nav>

<h1>{{title}}</h1>

<form>
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Nº de Matrícula</label>
    <input type="number" class="form-control">
  </div>
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Nome completo <i class="bi bi-trash"></i></label>
    <input type="text" class="form-control">
  </div>
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Data de nascimento</label>
    <input type="date" class="form-control">
  </div>
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Endereço de Email</label>
    <input type="email" class="form-control" placeholder="name@example.com">
  </div>
  <button type="submit" class="btn btn-primary">Criar Aluno</button>
</form>